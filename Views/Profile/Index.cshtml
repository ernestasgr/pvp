@using test.Models

@model AccountViewModel
@{
	ViewData["Title"] = "User Profile Page";
}

<h1>User information</h1>
<table class="table table-striped">
    <tr>
        <td>Nickname:</td>
        <td>@Model.Nickname</td>
    </tr>
    <tr>
        <td>Email:</td>
        <td>@Model.Email</td>
    </tr>
    <tr>
        <td>Hash:</td>
        <td>@Model.Hash</td>
    </tr>
    <tr>
        <td>Role:</td>
        <td>@Model.RoleName</td>
    </tr>
    <tr>
        <td>Salt:</td>
        <td>@Model.Salt</td>
    </tr>
</table>